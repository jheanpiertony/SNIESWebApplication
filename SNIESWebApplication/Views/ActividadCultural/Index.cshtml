@model IEnumerable<SNIESWebApplication.Models.ActividadCultural>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink(Recursos.Resources.CrearNuevo, "Create")
</p>

<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-10">
            @Html.ActionLink(Recursos.Resources.DescargarReporteExcel, "CrearPlantillaExcel", null, new { @class = "btn btn-success", id = "CrearPlantillaExcel" })
        </div>
    </div>
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CODIGO_IES)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NOMBRE_IES)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ANO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SEMESTRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COD_UNIDAD_ORGANIZACIONAL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UNIDAD_ORGANIZACIONAL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CODIGO_ACTIVIDAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVIDAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COD_TIPO_ACTIVIDAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TIPO_ACTIVIDAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_INICIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_FINAL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_PERIODO)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CODIGO_IES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOMBRE_IES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ANO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEMESTRE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COD_UNIDAD_ORGANIZACIONAL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UNIDAD_ORGANIZACIONAL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CODIGO_ACTIVIDAD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVIDAD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COD_TIPO_ACTIVIDAD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIPO_ACTIVIDAD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_INICIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_FINAL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_PERIODO)
            </td>
            <td>
                @Html.ActionLink(Recursos.Resources.Editar, "Edit", new { id = item.Id }) |
                @Html.ActionLink(Recursos.Resources.Detalles, "Details", new { id = item.Id }) |
                @Html.ActionLink(Recursos.Resources.Eliminar, "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
