@model IEnumerable<SNIESWebApplication.Models.ConvenioInternacional>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink(Recursos.Resources.CrearNuevo, "Create")
</p>

<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-10">
            @Html.ActionLink(Recursos.Resources.DescargarReporteExcel, "CrearPlantillaExcel", null, new { @class = "btn btn-success", id = "CrearPlantillaExcel" })
        </div>
    </div>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ANO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SEMESTRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CODIGO_CONVENIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ID_TIPO_CONVENIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TIPO_CONVENIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVIDAD_FORMACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVIDAD_INVESTIGACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVIDAD_EXTENSION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVIDAD_ADMINISTRATIVA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OTRAS_ACTIVIDADES_COOPERACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_INICIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_TERMINACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_PERIODO)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ANO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEMESTRE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CODIGO_CONVENIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ID_TIPO_CONVENIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIPO_CONVENIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVIDAD_FORMACION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVIDAD_INVESTIGACION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVIDAD_EXTENSION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVIDAD_ADMINISTRATIVA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OTRAS_ACTIVIDADES_COOPERACION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_INICIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_TERMINACION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_PERIODO)
            </td>
            <td>
                @Html.ActionLink(Recursos.Resources.Editar, "Edit", new { id = item.Id }) |
                @Html.ActionLink(Recursos.Resources.Detalles, "Details", new { id = item.Id }) |
                @Html.ActionLink(Recursos.Resources.Eliminar, "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
