@model IEnumerable<SNIESWebApplication.Models.Admitido>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<p>
    @Html.ActionLink(Recursos.Resources.CrearNuevo, "Create")
</p>

<div class="form-horizontal">
    <div class="form-group">
        <div class="col-md-10">
            @Html.ActionLink(Recursos.Resources.DescargarReporteExcel, "CrearPlantillaExcel", null, new { @class = "btn btn-success", id = "CrearPlantillaExcel" })
        </div>
    </div>
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID_IES)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NOMBRE_IES)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ANO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SEMESTRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COD_DANE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DEPARTAMENTO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MUNICIPIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRO_CONSECUTIVO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROGRAMA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TIPO_DOCUMENTO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NUMERO_DOCUMENTO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_PERIODO)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID_IES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOMBRE_IES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ANO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEMESTRE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.COD_DANE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DEPARTAMENTO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MUNICIPIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PRO_CONSECUTIVO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PROGRAMA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIPO_DOCUMENTO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NUMERO_DOCUMENTO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_PERIODO)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
