@model IEnumerable<SNIESWebApplication.Models.Periodo>

@{
    ViewBag.Title = Recursos.Resources.Index;
}

<h2>@Recursos.Resources.Index</h2>

<p>
    @Html.ActionLink(Recursos.Resources.CrearNuevo, "Create", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FechaPeriodo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FechaPeriodo)
        </td>
        <td>
            @Html.ActionLink(Recursos.Resources.Editar, "Edit", new { id=item.Id }, new { @class = "btn btn-warning" }) |
            @Html.ActionLink(Recursos.Resources.Detalles, "Details", new { id=item.Id }, new { @class = "btn btn-info" }) |
            @Html.ActionLink(Recursos.Resources.Eliminar, "Delete", new { id=item.Id }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
