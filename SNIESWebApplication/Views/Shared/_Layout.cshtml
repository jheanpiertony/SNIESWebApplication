                            <li>@Html.ActionLink("Primiparos", "Create", "EstudiantePrimerCurso")</li>
                            <li>@Html.ActionLink("Cupos", "Create", "Cupo")</li>
                            <li>@Html.ActionLink("Retiros", "Create", "RetiroDisciplinario")</li>
                            <li>@Html.ActionLink("Apoyos", "Create", "ApoyoEstudiante")</li>
                            <li>@Html.ActionLink("Estudiante de articulacion", "Create", "EstudianteArticulacion")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown">Bienestar<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Actividad bienestar", "Create", "ActividadBienestar")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown">Extensiones<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Actividad Cultural", "Create", "Admitido")</li>
                            <li>@Html.ActionLink("Consultoría", "Create", "Admitido")</li>
                            <li>@Html.ActionLink("Cursos", "Create", "Admitido")</li>
                            <li>@Html.ActionLink("Educación Continua", "Create", "Admitido")</li>
