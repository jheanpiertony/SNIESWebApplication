                            <li>@Html.ActionLink(" Derechos Pecuniarios IES Privadas ", "Create", "Admitido")</li>
                            <li>@Html.ActionLink(" Proyectos de Inversión ", "Admitido")</li>
                            <li>@Html.ActionLink(" Valores de Matrícula IES Privadas ", "Create", "Admitido")</li>
